visible fn min(a: Int, b: Int): Int = if (a <= b) a else b
visible fn max(a: Int, b: Int): Int = if (a >= b) a else b

visible fn abs(a: Int): Int = if (a < 0) -a else a

visible fn avg(nums: Array<Int>): Int {
    var sum = 0
    for (num in nums) {
      sum += num
    }
    return sum / len(nums)
}