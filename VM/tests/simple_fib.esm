# I think we did it!?

non_base
    Load 0
    Int 1
    Sub
    Enter_Frame
        Copy
        Scope fib Visit
    Exit_Frame
    Load 1  # return of fib(n - 1)

    Load 0
    Int 2
    Sub
    Enter_Frame
        Copy
        Scope fib Visit
    Exit_Frame
    Load 1  # return of fib(n - 2)

    # store return result
    Add
    Store 1
end

fib
    # arg 0
    # return 1
    Load 0
    Int 2
    Lesser_Than
    Scope non_base Gnq

    # return=n
    Load 0
    Store 1
end