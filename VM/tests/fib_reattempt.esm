fib_non_base
    # store scope's original n
    Load 0
    Mark_Scope

    # store n = [n - 1], call fib
    Copy
    Int 1
    Sub
    Store 0
    Scope fib Visit

    Revert_Scope
    Load 0      # load return result

    # store n = [n - 2], call fib
    Int 2
    Sub
    Store 0
    Scope fib Visit
    Load 0      # load return result

    # add both
    Add
    Store 0     # a new N
end

fib
    # memory 0 for n
    Load 0
    Int 2
    Lesser_Than
    Scope fib_non_base Gnq
end